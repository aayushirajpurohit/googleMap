{
  "name": "bkmortgage",
  "version": "1.2.4",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-8": "react-native run-ios --simulator 'iPhone 8'",
    "ios-12": "react-native run-ios --simulator 'iPhone 12'",
    "postinstall": "patch-package",
    "build:release": "cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postversion": "react-native-version --never-amend --skip-tag",
    "deploy": "npm version 1.0.0 --allow-same-version --skip-tag --never-amend -m \"Build & Deploy v%s\"",
    "pods": "cd ios && pod install && cd ..",
    "package-android": "cd android && ./gradlew bundleRelease -x bundleReleaseJsAndAssets && cd ..",
    "release-android": "cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && cd ..",
    "post_clone_script": "./custom-build.sh"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/BK Mortgage.app",
        "build": "xcodebuild -workspace 'ios/bkmortgage.xcworkspace' -scheme 'BKCO Mortgage' -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro Max"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_2_API_29"
        }
      }
    },
    "test-runner": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-community/toolbar-android": "^0.2.1",
    "@react-navigation/material-bottom-tabs": "^5.0.0-alpha.36",
    "@react-navigation/native": "^5.0.0-alpha.29",
    "@react-navigation/stack": "^5.0.0-alpha.63",
    "axios": "^0.19.2",
    "formik": "^2.1.2",
    "patch-package": "^6.2.2",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "~0.61.4",
    "react-native-auth0": "^2.1.1",
    "react-native-code-push": "^7.1.0",
    "react-native-dropdown-picker": "^3.6.8",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-masked-text": "^1.13.0",
    "react-native-material-ripple": "^0.9.1",
    "react-native-modal-dropdown": "^0.7.0",
    "react-native-paper": "^3.5.1",
    "react-native-picker-select": "^6.3.4",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.29",
    "react-native-share": "^3.0.0",
    "react-native-slider": "^0.11.0",
    "react-native-svg": "^10.1.0",
    "react-native-svg-icon": "^0.8.1",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.0",
    "throttle-debounce": "^2.1.0",
    "yarn": "^1.22.21",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "~24.9.0",
    "babel-plugin-inline-import": "^3.0.0",
    "detox": "^18.3.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "jest": "~24.9.0",
    "jetifier": "~1.6.4",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-unimodules": "^0.7.0",
    "react-native-version": "^3.2.0",
    "react-test-renderer": "~16.9.0"
  },
  "private": true,
  "resolutions": {
    "@babel/traverse": "7.16.7"
  }
}
